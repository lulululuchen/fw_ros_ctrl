<launch>
  <node name="nmpc" pkg="fw_ctrl" type="fw_nmpc"/>
  <param name="/nmpc/loop_rate" type="double" value="10.0" />
  <param name="/nmpc/time_step" type="double" value="0.1" />

  <arg name="fake_signals" default="false" doc="fake airspeed, wind speed, and ground speed during on-ground testing (true/false)" /> 
  <param name="/nmpc/fake_signals" type="int" value="$(arg fake_signals)"/>
  
  <param name="/nmpc/y_ref/uT" type="double" value="0.375" />
  <param name="/nmpc/y_ref/theta_ref" type="double" value="0.0222" />
  
  <param name="/nmpc/w_scale/q1" type="double" value="1.570796326794897" />
  <param name="/nmpc/w_scale/q2" type="double" value="1.570796326794897" />
  <param name="/nmpc/w_scale/q3" type="double" value="1.0" />
  <param name="/nmpc/w_scale/q4" type="double" value="0.872664625997165" />
  <param name="/nmpc/w_scale/q5" type="double" value="0.872664625997165" />
  <param name="/nmpc/w_scale/q6" type="double" value="0.872664625997165" />
  <param name="/nmpc/w_scale/q7" type="double" value="1.0" />
  <param name="/nmpc/w_scale/q8" type="double" value="1.0" />
  <param name="/nmpc/w_scale/q9" type="double" value="1.0" />
  <param name="/nmpc/w_scale/q10" type="double" value="0.523598775598299" />
  <param name="/nmpc/w_scale/q11" type="double" value="0.261799387799149" />
</launch>
